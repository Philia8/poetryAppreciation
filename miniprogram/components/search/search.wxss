/* 引入iconFont  */

@import "/static/icon/fontAwesome";

/* 搜索框 */
.searchContainer {
    height: 70rpx;
    /* width: 680rpx; */
    line-height: 70rpx;
    border: 2rpx #E6E6E6 solid;
    border-radius: 35rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    background-color: #fff;
}

.searchContainer .fa-search {
    color: #B6B6B6;
    font-size: 45rpx;
    margin-right: 10rpx;
}

.searchContainer .searchInput {
    width: 70%;
    line-height: 70rpx;
}

.searchContainer .btn-search{
    background-color: #f4f4f4;
    width: 150rpx;
    height: 100%;
    border-radius: 35rpx;
    text-align: center;
    font-weight: bolder;
}